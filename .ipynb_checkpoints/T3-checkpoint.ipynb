    "MonteCarlo <- function(n, f){\n",
    "    X <- runif(n)\n",
    "    Y <- f(X)\n",
    "    Intgr <- sum(Y)/n\n",
    "    Error <- Intgr-(pnorm(1)-pnorm(0))\n",
    "    list(\"Result=\" = Intgr,\"Error=\" = Error)\n",
    "}"
   ]
