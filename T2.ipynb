   "source": [
    "Integral <- function(n){\n",
    "    X <- runif(n)\n",
    "    Y <- (1/sqrt(2*pi))^(((X-2)^2)/2)\n",
    "    Intgr <- sum(Y)/n\n",
    "    Error <- Intgr-(pnorm(1)-pnorm(0))\n",
    "    list(Int,Error)\n",
    "}"
   ]
